{
  "name": "ai2o",
  "version": "1.0.0",
  "private": true,
  "description": "ai2o - AI-powered OSINT and productivity platform for organizations",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/judas-priest/ai2o.git"
  },
  "keywords": [
    "ai",
    "osint",
    "agents",
    "automation",
    "productivity"
  ],
  "author": "ai2o Team",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "typecheck": "pnpm -r typecheck",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "husky": "^8.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
